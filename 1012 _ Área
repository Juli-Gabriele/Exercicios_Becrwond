#include <iostream>
#include  <cmath>
using namespace std;
int main(){
    double a,b,c,area1,area2,area3,area4,area5;
    cin >>a >> b >> c;
    area1= (a*c)/2;
    area2= (3.14159 * pow(c,2));
    area3= ((a + b)* c)/2;
    area4=  pow(b,2);
    area5=  a * b;
    cout.precision(3);
    cout.setf(ios::fixed);
    cout <<"TRIANGULO: "<< area1 <<endl;
    cout <<"CIRCULO: "<< area2 <<endl;
    cout <<"TRAPEZIO: "<< area3 <<endl;
    cout <<"QUADRADO: "<< area4 <<endl;
    cout <<"RETANGULO: "<< area5 <<endl;
return 0;

}
